{
  "name": "company",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": " next start",
    "lint": "next lint",
    "test": "TZ=UTC jest",
    "localstack:seed": "ts-node --project tsconfig-codegen.json scripts/localstack-seed.ts",
    "db:generate-prisma": "prisma generate",
    "db:migrate": "prisma migrate dev --create-only",
    "db:up": "prisma migrate deploy",
    "db:reset": "yarn prisma migrate reset --force --skip-seed",
    "db:seed": "ts-node scripts/seed.ts",
    "db:view": "yarn prisma studio",
    "graphql:emit-schema": "ts-node --project tsconfig.json scripts/emit-schema.ts",
    "graphql:generate-client": "graphql-codegen --config codegen.ts",
    "sync": "yarn db:up && yarn db:generate-prisma && yarn graphql:emit-schema && yarn graphql:generate-client"
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@emotion/babel-preset-css-prop": "^11.11.0",
    "@genql/cli": "2.10.0",
    "@genql/runtime": "2.10.0",
    "@prisma/client": "^5.0.0",
    "e-prim": "^1.6.0",
    "graphql": "^16.6.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.22.4",
    "graphql-yoga": "^4.0.4",
    "next": "^13.1.6",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^29.1.1",
    "tslib": "^2.4.1",
    "type-graphql": "2.0.0-beta.2"
  },
  "prisma": {
    "schema": "src/db/schema.prisma"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/react": "^11.11.1",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/graphql-fields": "^1.3.4",
    "@types/jest": "^29.5.4",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "dotenv": "^16.0.3",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-storybook": "^0.6.10",
    "jest": "^29.7.0",
    "prettier": "^2.8.4",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
    "typegraphql-prisma": "^0.27.0",
    "typescript": "^5.1.6"
  }
}
